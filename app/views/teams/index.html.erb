<%= image_tag('pear-programming.png') %>

<br />

<table>
 <tr>
   <th>Team Name</th>
   <th>Team Members</th>
 </tr>
 <% @teams.each do |team| %>
  <tr>
    <td><a href='<%= url_for(team) %>'><%= team.name %></a></td>
    <td><%= team.people.count %></td>
  </tr>
 <% end %>
</table>

<br /><br />

Add Team
<%= form_for :new_team do |f| %>
  Team Name: <%= f.text_field :name %>
  <%= f.submit :Submit %>
<% end %>